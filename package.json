{
  "name": "cloud189-sdk",
  "version": "1.0.7",
  "description": "基于node.js的第三方天翼云盘SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!__tests__"
  ],
  "scripts": {
    "start": "ts-node ./example/app.ts",
    "build": "tsc",
    "docs:build": "npm run build && cd docs && npm run build",
    "docs:dev": "npm run build && cd docs && npm run dev",
    "test": "node --security-revert=CVE-2023-46809 ./node_modules/mocha/bin/mocha",
    "coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov > coverage/lcov.info"
  },
  "author": "wes lin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wes-lin/cloud189-sdk.git"
  },
  "keywords": [
    "天翼网盘sdk"
  ],
  "license": "MIT",
  "dependencies": {
    "@netdrive-sdk/log": "1.0.0",
    "got": "11.8.2"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.13",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "4.4.1",
    "coveralls": "^3.1.1",
    "mocha": "^10.4.0",
    "nock": "14.0.0-beta.7",
    "nyc": "^15.1.0",
    "prettier": "^3.3.2",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5"
  }
}
